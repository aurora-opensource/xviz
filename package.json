{
  "name": "xviz",
  "description": "Utility library for handling XVIZ protocol",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/xviz.git"
  },
  "workspaces": [
    "modules/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta",
    "@babel/core": "^7.0.0-beta",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta",
    "@babel/preset-env": "^7.0.0-beta",
    "@babel/register": "^7.0.0-beta",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-minify": "^0.4.0-alpha.caaefb4c",
    "coveralls": "^2.13.0",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-uber-es2015": "^3.1.2",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^2.9.1",
    "module-alias": "^2.0.0",
    "nyc": "^10.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "prettier-check": "^2.0.0",
    "source-map-loader": "^0.2.1",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.9",
    "tape-catch": "^1.0.4",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webworkify-webpack": "^2.0.0"
  },
  "scripts": {
    "bootstrap": "scripts/bootstrap.sh",
    "bench": "NODE_ENV=test node test/start bench",
    "build": "lerna run build",
    "bump": "lerna publish --force-publish=* --skip-npm --skip-git && npm version patch --force",
    "flow": "flow check",
    "cover": "nyc npm run test",
    "lint": "scripts/lint.sh",
    "publish": "scripts/publish.sh",
    "test": "npm run lint && NODE_ENV=test node test/start test",
    "test-browser": "webpack-dev-server --config test/webpack.config.js --hot --open",
    "version": "git add ."
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5",
    "yarn": "1.7.0"
  },
  "pre-commit": [
    "test"
  ],
  "private": true
}
