{
  "$id": "https://xviz.org/schema/core/future_instances.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "XVIZ Future Instances",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "timestamps": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "number",
        "minimum": 0
      }
    },
    "primitives": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "https://xviz.org/schema/core/_primitive_list.schema.json"
      }
    }
  },
  "required": [
    "name",
    "timestamps",
    "primitives"
  ],
  "additionalProperties": false
}
